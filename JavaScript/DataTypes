// Premitive data Types
// string
let name = "Tausif";
console.log("My string is " + name);
console.log("My Data type is " + typeof (name));

// Numbers
let marks = 34;
console.log("My Data type is " + typeof (marks));

// Boolean
let isOk = true;
console.log("My Data type is " + typeof (isOk));

// Null
let nullVar = null;
console.log("My Data type is " + typeof (nullVar));

// undefined
let undef = undefined;
console.log("My Data type is " + typeof (undef));

// Referenced Data Type / Non-premitive

// Arrays
let myarr = [1, 2, 3,];
console.log("My Data type is " + typeof (myarr));

// Object Literals
let score = {
    tausif: 50,
    bunny: 45,
}
console.log(score);
